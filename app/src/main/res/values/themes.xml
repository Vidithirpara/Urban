<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Urban" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.Urban" parent="Base.Theme.Urban" />

    <style name="poppinsMediumText">
        <item name="fontFamily">@font/popins_medium</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="poppinsRegularText">
        <item name="fontFamily">@font/popins_regular</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="ralewayBoldText">
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">@dimen/_15ssp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <string-array name="spinner_gender">
        <item>-Select one-</item>
        <item>Male</item>
        <item>Female</item>
        <item>Custom</item>
    </string-array>

    <string-array name="spinner_category2">
        <item>-Select one-</item>
        <item>Category_1</item>
        <item>Category_2</item>
        <item>Category_3</item>
    </string-array>

    <string-array name="spinner_state">
        <item>-Select one-</item>
        <item>State-1</item>
        <item>State-2</item>
        <item>State-3</item>
    </string-array>

    <string-array name="spinner_city">
        <item>-Select one-</item>
        <item>City-1</item>
        <item>City-2</item>
        <item>City-3</item>
    </string-array>

</resources>